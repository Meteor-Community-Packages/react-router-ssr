{
  "extends": ["standard", "standard-react"],
  "rules": {
    "semi": [2, "always"],
    "comma-dangle": [2, "always-multiline"],
    "react/prop-types": 0
  },
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "settings": {
    "import/resolver": {
      "meteor": {
        "extensions": [".js", ".jsx"]
      }
    }
  }
}
